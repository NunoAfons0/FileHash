<Window x:Class="fhash.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="fhash" Height="420" Width="550" Icon="/fhash;component/Images/tree.png" Closed="Window_Closed">
	<Grid>
		<TextBox Margin="12,101,12,20" Name="textBox1" PreviewDragOver="textBox1_PreviewDragOver" PreviewDrop="textBox1_PreviewDrop" TextWrapping="WrapWithOverflow" FontFamily="Consolas" Foreground="#FF363636" VerticalScrollBarVisibility="Auto" Text="Drop Files Here!..."></TextBox>
		<GroupBox Header="Hash" Height="83" HorizontalAlignment="Left" Margin="12,12,0,0" Name="groupBox1" VerticalAlignment="Top" Width="213">
			<Grid>
				<CheckBox Content="SHA-512" Height="16" HorizontalAlignment="Left" Margin="10,16,0,0" Name="checkBox1" VerticalAlignment="Top" IsChecked="True" IsEnabled="True" />
				<CheckBox Content="SHA-256" Height="16" HorizontalAlignment="Left" Margin="118,16,0,0" Name="checkBox2" VerticalAlignment="Top" />
				<CheckBox Content="SHA-128" Height="16" HorizontalAlignment="Left" Margin="10,38,0,0" Name="checkBox3" VerticalAlignment="Top" />
				<CheckBox Content="MD5" Height="16" HorizontalAlignment="Left" Margin="118,38,0,0" Name="checkBox4" VerticalAlignment="Top" />
			</Grid>
		</GroupBox>
	</Grid>
</Window>
